<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.metacoding.firstapp.domain.img.ImgDao">

	<insert id="save"> INSERT into img (title, imgName, content)
		VALUES(#{title},#{imgName},#{content}) </insert>

	<select id="findById" resultType="site.metacoding.firstapp.domain.img.Img"> SELECT * FROM img
		WHERE id = ${id} </select>

	<update id="update"> </update>

</mapper>